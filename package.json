{
  "name": "es5-project-template",
  "version": "0.0.0",
  "description": "a small example of code and test structure",
  "main": "index.js",
  "scripts": {
    "coverage": "istanbul cover _mocha test/**/*_spec.*",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint lib/ test/",
    "test": "mocha test/**/*_spec.*",
    "test-watch": "npm test -- --watch",
    "ci": "npm run lint && npm run coverage && npm run coveralls"
  },
  "keywords": [
    "tests",
    "js",
    "example"
  ],
  "author": "seanstrom",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.11.4",
    "eslint": "^1.3.1",
    "expect": "^1.9.0",
    "istanbul": "^0.3.19",
    "mocha": "^2.3.2",
    "selenium-standalone": "^4.6.0",
    "webdriverio": "^3.2.4",
    "webdriverio-selenium-harness": "^1.0.0"
  },
  "dependencies": {
    "connect": "^3.4.0",
    "serve-static": "^1.10.0"
  }
}
