{
  "name": "es5-project-template",
  "version": "0.0.0",
  "description": "a small example of code and test structure",
  "main": "src/index.js",
  "scripts": {
    "ci": "npm run lint && npm run coverage && npm run codeclimate && npm run coveralls && npm run codecov",
    "codeclimate": "npm run lcov | codeclimate-test-reporter",
    "codecov": "cat ./coverage/coverage.json | codecov.io.js",
    "coverage": "istanbul cover _mocha test/**/*.spec.*",
    "coveralls": "npm run lcov | coveralls",
    "features": "mocha test/features/**/*.spec.*",
    "lcov": "cat ./coverage/lcov.info",
    "lint": "eslint src/ test/",
    "postinstall": "selenium-standalone install",
    "specs": "mocha test/specs/**/*.spec.*",
    "test": "mocha test/**/*.spec.*",
    "test-watch": "npm test -- --watch"
  },
  "keywords": [
    "tests",
    "js",
    "example"
  ],
  "author": "seanstrom",
  "license": "MIT",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.1.1",
    "codecov.io": "^0.1.6",
    "coveralls": "^2.11.4",
    "eslint": "^1.3.1",
    "expect": "^1.9.0",
    "istanbul": "^0.3.19",
    "mocha": "^2.3.2",
    "selenium-standalone": "^4.6.0",
    "webdriverio": "^3.2.4",
    "webdriverio-selenium-harness": "^1.0.0"
  },
  "dependencies": {
    "connect": "^3.4.0",
    "serve-static": "^1.10.0"
  }
}
